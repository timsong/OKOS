@model WFS.WebSite4.Areas.Admin.Models.SearchResultsViewModel


<div class="panel">
    <h5>Search Results</h5>
    @foreach (var x in Model.Results)
    {
        <div class="row p_t5">
            <div class="three columns">
               <label class="inline">@x.DisplayName</label>
            </div>
            <div class="three columns">
                <label class="inline">@x.EmailAddress</label>
            </div>
            <div class="three columns">
                <label class="inline">@x.AccountType</label>
            </div>
            <div class="three columns end">
		        <a href="javascript:void(0)" msclick="usersearch.getDetails" data-id="@x.UserId" class="button radius small">Edit</a>
		        <a href="javascript:void(0)" msclick="usersearch.loginAs" data-id="@x.UserId" class="button radius small">Login As</a>
            </div>
        </div>    
    }
</div>
